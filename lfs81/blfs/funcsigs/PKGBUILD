BLFSTOOLSEQ=532
pkgname=funcsigs
pkgver=1.0.2
pkgrel=1
# EDITHOOK2
pkgdesc=""
arch=('x86_64')
# EDITHOOK3
url=""
license=('GPL')
groups=()
# EDITHOOK4
depends=()
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
# EDITHOOK5
source=(
    https://files.pythonhosted.org/packages/source/f/funcsigs/funcsigs-1.0.2.tar.gz
)
# EDITHOOK6
noextract=()
# EDITHOOK7

md5sums=('7e583285b1fb8a76305d6d68f4ccc14e')

# EDITHOOKPREP
prepare() {
	cd "$pkgname-$pkgver"
	patch -p1 -i "$srcdir/$pkgname-$pkgver.patch"
}

# EDITHOOKBLD
build() {
	cd "$pkgname-$pkgver"
	./configure --prefix=/usr
	make
}

# EDITHOOKCHK
check() {
	cd "$pkgname-$pkgver"
	make -k check
}

# EDITHOOKPKG
package() {
	cd "$pkgname-$pkgver"
	make -j1 DESTDIR="$pkgdir/" install
}
# EDITHOOKC
#######  Building package
cd $UNPACKDIR

sudo -E sh << ROOT_EOF
python setup.py install --optimize=1
ROOT_EOF
sudo /sbin/ldconfig


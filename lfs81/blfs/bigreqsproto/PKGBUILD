# EDITHOOKPREP
prepare() {
	cd "$pkgname-$pkgver"
	./configure $XORG_CONFIG
}

# EDITHOOKBLD
build() {
	cd "$pkgname-$pkgver"
}

# EDITHOOKCHK
check() {
	cd "$pkgname-$pkgver"
}

# EDITHOOKPKG
package() {
	cd "$pkgname-$pkgver"
	make -j1 DESTDIR="$pkgdir/" install
}
